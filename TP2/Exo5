Option Explicit
Private Sub exo5()
 
Dim n%
Dim m%
Dim res&(100)
Dim af$
Dim i%

Do
n = CInt(InputBox("saisir n positif ", "saisie"))
m = CInt(InputBox("Saisir m positif ", "saisie"))
Loop While Not (m > 0 And m - n <= 100 And m > n)

For i = 0 To m - n Step 1
    res(i) = (n + i) ^ 3
Next

af = "les carr√©s : "

For i = 0 To m - n Step 1
    af = af & vbCr & i & n & " ^3 = " & res(i)
Next

Call MsgBox(af, vbInformation, "resultat")

End Sub
